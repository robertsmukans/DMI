x = input("ievadiet x: ")
print "x = ", (x)
print " \t%1  \t%2  \t%3  \t%4  \t%5  \t%6  \t%7  \t%8  \t%9  \t%10"
k=0
while k < 11:
    a = x%1
    b = x%2 
    c = x%3
    d = x%4
    e = x%5
    f = x%6
    g = x%7
    h = x%8
    i = x%9
    j = x%10
    print (x),  "\t", (a), "\t", (b), "\t", (c), "\t", (d), "\t", (e), "\t", (f), "\t", (g), "\t", (h), "\t", (i), "\t", (j)
    k=k+1
x=x+1
